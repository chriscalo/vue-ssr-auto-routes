{
  "name": "vue-ssr",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "generate-router": "node src/generate-routes.js",
    "ssr:build": "yarn run generate-router && vue-cli-service ssr:build",
    "ssr:serve": "yarn run generate-router && vue-cli-service ssr:serve",
    "ssr:start": "cross-env NODE_ENV=production vue-cli-service ssr:serve --mode production"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.6",
    "vue-route-generator": "^0.3.3",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.6.0"
  },
  "devDependencies": {
    "@akryum/vue-cli-plugin-ssr": "^0.5.0",
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  }
}
